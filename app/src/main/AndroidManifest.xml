<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".horoscopo.HoroscopoApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CursoAndroid"
        tools:ignore="ExtraText"
        tools:targetApi="31">
        <activity
            android:name=".horoscopo.ui.home.HoroscopoActivity"
            android:exported="false" />
        <activity
            android:name=".configuraciones.ConfiguracionesActivity"
            android:exported="false" />
        <activity
            android:name=".heroes.DetallesHeroeActivity"
            android:exported="false" />
        <activity
            android:name=".recyclerView.EjemploRecyclerViewActivity"
            android:exported="false" />
        <activity
            android:name=".heroes.ListaHeroesActivity"
            android:exported="false" />
        <activity
            android:name=".tareasApp.TareasActivity"
            android:exported="false"
            android:theme="@style/Theme.TareasApp" />
        <activity
            android:name=".imcApp.ResultadoActivity"
            android:exported="true" />
        <activity
            android:name=".MenuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ejerciciosLayout.ConstraintLayoutActivity"
            android:exported="true" />
        <activity
            android:name=".ejerciciosLayout.LinearLayoutActivity"
            android:exported="true" />
        <activity
            android:name=".ejerciciosLayout.FrameLayoutActivity"
            android:exported="true" />
        <activity
            android:name=".primeraApp.primeraAppActivity"
            android:exported="true" />
        <activity
            android:name=".primeraApp.SaludoActivity"
            android:exported="true" />
        <activity
            android:name=".imcApp.imcCalculadorActivity"
            android:exported="true" />
    </application>

</manifest>